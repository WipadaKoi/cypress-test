/// <reference types="Cypress" />

describe('Cypress Test', () => {
    beforeEach(() => {
        cy.visit('https://www.cypress.io')
    })

    it('should has text \'npm install cypress\'', () => {
        cy.get('[class="with-bash-effects undefined NpmButton__Button-sc-8gct4l-0 dYXwUi"]')
        .should('contain', 'npm install cypress')
    })
})
